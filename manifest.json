{
  "name": "Tab Box",
  "description": "Tab Manager",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "tabs"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "icons": {
    "128": "logo.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://kit.fontawesome.com/11f67e7c0a.js https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js; object-src 'self'"
}